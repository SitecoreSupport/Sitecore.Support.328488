<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <pipelines>
      <httpRequestBegin>
        <processor patch:instead="*[@type='Sitecore.XA.Feature.Redirects.Pipelines.HttpRequest.RedirectMapResolver, Sitecore.XA.Feature.Redirects']" type="Sitecore.Support.XA.Feature.Redirects.Pipelines.HttpRequest.RedirectMapResolver, Sitecore.Support.328488" resolve="true">
          <CacheExpiration>720</CacheExpiration>
        </processor>
      </httpRequestBegin>
    </pipelines>
  </sitecore>
</configuration>